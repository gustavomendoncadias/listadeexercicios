programa {
  funcao inicio() {
    Algoritmo VogalOuConsoante
Var
   letra: Caracter

Inicio
   escreva("Digite uma letra: ")
   leia(letra)

   Se letra = "a" Ou letra = "e" Ou letra = "i" Ou letra = "o" Ou letra = "u" Ou
      letra = "A" Ou letra = "E" Ou letra = "I" Ou letra = "O" Ou letra = "U" Entao
      escreva("A letra é uma vogal.")
   Senao
      escreva("A letra é uma consoante.")
   FimSe

FimAlgoritmo

  }
}
