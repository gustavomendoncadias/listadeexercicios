programa {
  funcao inicio() {
    algoritmo "ImprimirNumeros"
var
   i: inteiro

inicio
   // Imprime os números um abaixo do outro
   escreva("Números um abaixo do outro:")
   para i de 1 ate 20 faca
      escreval(i)
   fimpara

   // Imprime os números um ao lado do outro
   escreva("Números um ao lado do outro:")
   para i de 1 ate 20 faca
      escreva(i, " ")
   fimpara
fimalgoritmo

  }
}
