programa {
  funcao inicio() {
    Algoritmo Multiplicacao
Var
   numero1, numero2, resultado: Real

Inicio
   escreva("Digite o primeiro número: ")
   Leia(numero1)
   
   escreva("Digite o segundo número: ")
   Leia(numero2)
   
   resultado <- numero1 * numero2
   
   escreva("A multiplicação de ", numero1, " e ", numero2, " é: ", resultado)
Fim

  }
}
