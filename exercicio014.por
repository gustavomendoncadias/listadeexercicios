programa {
  funcao inicio() {
    algoritmo "TabuadaDo8"
var
   multiplicador, resultado: inteiro

inicio
   // Loop para calcular e exibir a tabuada do 8
   para multiplicador de 1 ate 10 faca
      resultado <- 8 * multiplicador
      escreva("8 x ", multiplicador, " = ", resultado)
      escreval() // Pula para a próxima linha após cada multiplicação
   fimpara
fimalgoritmo

  }
}
